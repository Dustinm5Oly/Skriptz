

    <!-- Button trigger modal -->
<button type="button" id="createBtn" class="btn" data-bs-toggle="modal" data-bs-target="#staticBackdrop">
  Custom Subscription
</button>

<!-- Modal -->
<div class="modal fade" id="staticBackdrop" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" aria-labelledby="staticBackdropLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <h1 class="modal-title fs-5" id="staticBackdropLabel">Custom Subscription</h1>
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
      </div>
      <div class="modal-body">
    <form id="addserviceForm">
        <div class="form-group">
         <label for="inputName">Name:</label>
            <input type="text" class="form-control" id="inputName" aria-describedby="emailHelp" placeholder="Enter Subscription Name">
        </div>
        <div class="form-group">
         <label for="inputPrice">Price:</label>
         <input type="number" step="0.01" min="0" class="form-control" id="inputPrice" placeholder="$9.99">
        </div>
        <div class="form-group">
         <label for="inputDue">Due Date:</label>
         <input type="number" class="form-control" id="inputDue" placeholder="">
        </div>
        <div class="form-group col-md-4">
            <label for="inputCat">Category</label>
            <select id="inputCat" class="form-control">
            <option selected>Choose...</option>
            <option>TV/Movies</option>
            <option>Music</option>
            <option>Gaming</option>
            <option>Personal</option>
            </select>
        </div>
        <div class="form-group">
         <label for="inputSubName">Account Username:</label>
            <input type="text" class="form-control" id="inputSubName" >
        </div>
        <div class="form-group">
         <label for="inputpassHelp">Password Hint:</label>
            <input type="text" class="form-control" id="inputpassHint">
        </div>
         <button type="submit" id="addserviceSubmit" class="btn btn-primary">Submit</button>
    </form>

      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
      </div>
    </div>
  </div>
</div>


